
#include <iostream>
using namespace std;

int main() {
    setlocale(0, "");
    string pwd;
    cout << "Введите пароль: ";
    while (cin >> pwd) {
        if (pwd == "12345") {
            cout << "Добро пожаловать!" << endl;
            break;
        }
        else {
            cout << "Неверный пароль!" << endl;
            cout << "Введите пароль: ";
        }
    }
    return 0;
}
